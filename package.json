{
  "name": "syanflower",
  "version": "0.1.3",
  "description": "powerful ajax tool that support native cache request",
  "main": "index.js",
  "scripts": {
    "build": "gulp"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/superstring/syanflower"
  },
  "keywords": [
    "ajax",
    "cached"
  ],
  "author": "K.Arthur",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.21.1",
    "localforage": "^1.7.3",
