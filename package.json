    "url-search-params-polyfill": "^5.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "delete": "^1.1.0",
    "eslint": "^5.16.0",
    "eslint-plugin-html": "^5.0.5",
    "gulp": "^4.0.0",
    "gulp-babel": "^8.0.0",
    "gulp-rename": "^1.4.0",
    "gulp-uglify": "^3.0.2",
    "rollup": "^1.9.2",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-node-resolve": "^4.2.2"
  },
  "buildConfig": {
